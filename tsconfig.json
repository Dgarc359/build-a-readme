{
    "compilerOptions": {
        "strict": true,
        "module": "commonjs",
        "target": "es6",
        "moduleResolution": "node",
        "outDir": "out",
        "resolveJsonModule": false,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "sourceMap": true,
    }
}
